modu std {
    typ oth {
        other: string;
        ctor(other);
    }
	typ cmd {
		arg, val: string;
		ctor(arg, val);
		
		fun init() {
		    arg = *std::oth('hello world from typ oth');
		}
	}

	typ argparse {
		fun parse() {
			var en = *env();
			var args = en.get_args();
			var cmds = [];
			#println(args.len());
			if(args.len() == 1) {
				ret cmds;
			}
			if(args.len() == 2) {
				cmds.add(*std::cmd(args[1], ""));
				ret cmds;
			}
			for (0..(args.len() - 1)) {
			    cmds.add(*std::cmd(args[it], ""));
				#println(args[it]);
			}

			ret cmds;
		}
	}
}


